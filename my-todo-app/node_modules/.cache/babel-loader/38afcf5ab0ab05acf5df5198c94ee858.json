;AACA,WAAA,QAAA,CAAA,KAAA,QAAA,EADA,CAEA;;AACA,WAAA,QAAA,GAAA;AACA,QAAA,IAAA,EAAA,IADA;AAEA,QAAA,KAAA,EAAA;AAFA,OAAA;AAIA;AARA,KASA,UAAA,CAAA,CAAA,UAAA,CAAA,CATA;AApBA,CAAA","sourcesContent":["<template>\n  <v-container>\n    <h2>Your boards</h2>\n    {{this.$store.state.login_user}}\n    <v-container class=\"d-flex\">\n      <v-card\n        v-for=\"(board, index) in boards\"\n        :key=\"index\"\n        :to=\"{\n          name: 'board',\n          params: { slug: board.name },\n        }\"\n        class=\"ma-2\"\n        width=\"200\"\n        height=\"125\"\n        hover\n        dark\n        link\n        color=\"cyan lighten-2\"\n      >\n        <v-card-title v-text=\"board.name\"></v-card-title>\n      </v-card>\n    </v-container>\n    <h2>Create a new board</h2>\n    <v-container>\n      <v-flex xs4 mt-5>\n        <v-card>\n          <v-card-text>\n            <ValidationObserver v-slot=\"{ invalid }\">\n              <v-form>\n                <ValidationProvider rules=\"required\" v-slot=\"{ errors, valid }\" name=\"ボードタイトル\">\n                  <v-text-field\n                    outlined\n                    v-model=\"newBoard.name\"\n                    label=\"ボードタイトル\"\n                    color=\"grey\"\n                    :error-messages=\"errors\"\n                    :success=\"valid\"\n                  ></v-text-field>\n                </ValidationProvider>\n                <v-btn\n                  :disabled=\"invalid\"\n                  color=\"green lighten-2\"\n                  dark\n                  class=\"ml-2\"\n                  @click=\"createNewBoard\"\n                >新しいボードを作成</v-btn>\n              </v-form>\n            </ValidationObserver>\n          </v-card-text>\n        </v-card>\n      </v-flex>\n    </v-container>\n  </v-container>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\nimport { required } from \"vee-validate/dist/rules\";\nimport ja from \"vee-validate/dist/locale/ja.json\";\nimport {\n  extend,\n  localize,\n  ValidationProvider,\n  ValidationObserver,\n} from \"vee-validate\";\n\n// バリデーションルール\nextend(\"required\", required);\n// Localization\nlocalize(\"ja\", ja);\n\nexport default {\n  components: {\n    ValidationProvider,\n    ValidationObserver,\n  },\n  data() {\n    return {\n      results: [],\n      newBoard: {\n        name: null,\n        tiles: [],\n      },\n    };\n  },\n  computed: {\n    boards() {\n      return this.boards;\n    },\n    ...mapGetters([\"boards\"]),\n  },\n  methods: {\n    createNewBoard() {\n      this.addBoard(this.newBoard);\n      // 初期化\n      this.newBoard = {\n        name: null,\n        tiles: [],\n      };\n    },\n    ...mapActions([\"addBoard\"]),\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}