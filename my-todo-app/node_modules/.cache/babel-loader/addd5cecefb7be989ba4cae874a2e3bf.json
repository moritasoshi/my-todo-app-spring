-container class=\"d-flex\">\n      <v-card\n        v-for=\"(board, index) in boards\"\n        :key=\"index\"\n        :to=\"{\n          name: 'board',\n          params: { slug: board.board_name },\n        }\"\n        class=\"ma-2\"\n        width=\"200\"\n        height=\"125\"\n        hover\n        dark\n        link\n        color=\"cyan lighten-2\"\n      >\n        <v-card-title v-text=\"board.board_name\"></v-card-title>\n      </v-card>\n    </v-container>\n    <h2>Create a new board</h2>\n    <v-container>\n      <v-flex xs4 mt-5>\n        <v-card height=\"125\">\n          <v-card-text>\n            <v-form>\n              <v-text-field\n                outlined\n                required\n                v-model=\"newBoard.board_name\"\n                label=\"ボードタイトルを追加\"\n                color=\"grey\"\n              ></v-text-field>\n            </v-form>\n          </v-card-text>\n        </v-card>\n      </v-flex>\n    </v-container>\n    <v-container>\n      <v-btn\n        :disabled=\"\n          this.newBoard.board_name == null || this.newBoard.board_name == ''\n        \"\n        color=\"green lighten-2\"\n        dark\n        class=\"ml-2\"\n        @click=\"createNewBoard\"\n        >新しいボードを作成</v-btn\n      >\n    </v-container>\n  </v-container>\n</template>\n\n<script>\nimport { mapActions } from \"vuex\";\nexport default {\n  data() {\n    return {\n      newBoard: {\n        board_name: null,\n        tiles: [],\n      },\n    };\n  },\n  computed: {\n    boards() {\n      return this.$store.state.boards;\n    },\n  },\n  methods: {\n    createNewBoard() {\n      this.addBoard(this.newBoard);\n      // 初期化\n      this.newBoard = {\n        board_name: null,\n        tiles: [],\n      };\n    },\n    ...mapActions([\"addBoard\"]),\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}